{
    "version": 2,
    "functions":{
      "api/app.js":{
        "source": "src/routes/api.js",
"memory":1024,
"maxDuration":60
      }
    },
    "routes": [
      {
        "handle":"filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "api/app.js"
      }
    ],
    "regions": ["cdg1"],
    "installCommand": "npm install"
  }